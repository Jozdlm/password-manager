  <main>
    <h1>Contrase√±as</h1>
    {data.map((item: any) => <p>{item}</p>)}
  </main>
</Layout>

<style>
  main {
    margin: auto;
    padding: 1rem;
    width: 800px;
    max-width: calc(100% - 2rem);
    color: white;
    font-size: 20px;
    line-height: 1.6;
  }
</style>
